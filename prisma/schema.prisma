// This is your Prisma schema file
datasource db {
  provider = "sqlite"
  url      = "file:./dev.sqlite3"
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id        String   @id @default(cuid())
  name      String
  email     String   @unique
  password  String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
